graph TD
    A["Start nearest(p)"] --> B{isEmpty()};
    B -- True --> C[Return null];
    B -- False --> D[minDistance = Infinity];
    D --> E[For each point in set];
    E --> F["distance = point.distanceSquaredTo(p)"];
    F --> G{distance < minDistance};
    G -- True --> H[minDistance = distance, nearest = point];
    G -- False --> I;
    H --> I[More points?];
    I -- True --> E;
    I -- False --> J[Return nearest];
    C --> K[End];
    J --> K;
